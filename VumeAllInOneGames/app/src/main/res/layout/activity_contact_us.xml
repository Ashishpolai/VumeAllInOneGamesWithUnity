<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/splash_screen_bg"
    tools:context=".ContactUsActivity">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/topGuildeLine"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.05" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/leftGuideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.12" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/rightGuiudeline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.88" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/bottomGuideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.96998125" />

            <ImageView
                android:id="@+id/heading_laypout"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:background="@drawable/orangyyellow_bottomradius_bg"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.15"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0" />

            <TextView
                android:id="@+id/heading"
                style="@style/LargeSubHeading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contact_us_heading"
                android:textSize="@dimen/pixel_19dp"
                app:layout_constraintBottom_toTopOf="@id/bottomGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintTop_toTopOf="@id/topGuildeLine"
                app:layout_constraintVertical_bias="0.01" />

            <ImageView
                android:id="@+id/btn_back"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:layout_marginEnd="@dimen/pixel_8dp"
                android:src="@mipmap/left_arrow"
                app:layout_constraintBottom_toBottomOf="@+id/heading"
                app:layout_constraintEnd_toStartOf="@+id/rightGuiudeline"
                app:layout_constraintHeight_percent="0.02"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toEndOf="@id/leftGuideline"
                app:layout_constraintTop_toTopOf="@+id/heading"
                app:layout_constraintWidth_percent="0.08" />

            <TextView
                android:id="@+id/faq_subheading"
                style="@style/LargeSubHeading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frequently_asked_questions_subheading"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                app:layout_constraintTop_toBottomOf="@id/heading_laypout"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <TextView
                android:id="@+id/faq_one"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/faq_one"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/faq_subheading"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/faq_one_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/faq_one"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintTop_toTopOf="@id/faq_one"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_faqone"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/faq_one"
                app:layout_constraintBottom_toBottomOf="@id/faq_one"
                />

            <TextView
                android:id="@+id/faq_two"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/faq_two"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/btn_faqone"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/faq_two_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/faq_two"
                app:layout_constraintTop_toTopOf="@+id/faq_two"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_faqtwo"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/faq_two"
                app:layout_constraintBottom_toBottomOf="@id/faq_two"
                />

            <TextView
                android:id="@+id/faq_three"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/faq_three"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/btn_faqtwo"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/faq_three_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/faq_three"
                app:layout_constraintTop_toTopOf="@+id/faq_three"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_faqthree"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/faq_three"
                app:layout_constraintBottom_toBottomOf="@id/faq_three"
                />

            <TextView
                android:id="@+id/faq_four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/faq_four"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/btn_faqthree"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/faq_four_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/faq_four"
                app:layout_constraintTop_toTopOf="@+id/faq_four"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_faqfour"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/faq_four"
                app:layout_constraintBottom_toBottomOf="@id/faq_four"
                />

            <TextView
                android:id="@+id/faq_five"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/faq_five"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/faq_four"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/faq_five_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/faq_five"
                app:layout_constraintTop_toTopOf="@+id/faq_five"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_faqfive"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/faq_five"
                app:layout_constraintBottom_toBottomOf="@id/faq_five"
                />

            <View
                android:id="@+id/divider"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_1dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toBottomOf="@+id/btn_faqfive"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                android:background="@color/caper_six"
                app:layout_constraintVertical_bias="0.07"
                />

            <TextView
                android:id="@+id/activeissues_subheading"
                style="@style/LargeSubHeading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/active_issues_subheading"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                app:layout_constraintTop_toBottomOf="@id/divider"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.07" />

            <TextView
                android:id="@+id/issues_one"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="wrap_content"
                android:text="@string/issues_one"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/activeissues_subheading"
                app:layout_constraintEnd_toStartOf="@id/issues_one_arrow"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintWidth_percent="1"
                android:lineHeight="@dimen/pixel_22dp"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/issues_one_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/issues_one"
                app:layout_constraintTop_toTopOf="@+id/issues_one"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_issuesone"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/issues_one"
                app:layout_constraintBottom_toBottomOf="@id/issues_one"
                />

            <TextView
                android:id="@+id/issues_two"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="wrap_content"
                android:text="@string/issues_two"
                android:textColor="@color/secondarygreyone"
                android:textSize="@dimen/pixel_17dp"
                android:fontFamily="@font/rubikregular"
                app:layout_constraintTop_toBottomOf="@id/issues_one"
                app:layout_constraintEnd_toStartOf="@id/issues_two_arrow"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                app:layout_constraintWidth_percent="1"
                android:lineHeight="@dimen/pixel_22dp"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintVertical_bias="0.055" />

            <ImageView
                android:id="@+id/issues_two_arrow"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                android:src="@mipmap/icon_rightarrow_red"
                app:layout_constraintBottom_toBottomOf="@+id/issues_two"
                app:layout_constraintTop_toTopOf="@+id/issues_two"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintWidth_percent="0.07"
                app:layout_constraintHeight_percent="0.02"
                />

            <View
                android:id="@+id/btn_issuestwo"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_0dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toTopOf="@+id/issues_two"
                app:layout_constraintBottom_toBottomOf="@id/issues_two"
                />

            <View
                android:id="@+id/divider_two"
                android:layout_width="@dimen/pixel_0dp"
                android:layout_height="@dimen/pixel_1dp"
                app:layout_constraintStart_toStartOf="@id/leftGuideline"
                app:layout_constraintEnd_toEndOf="@id/rightGuiudeline"
                app:layout_constraintTop_toBottomOf="@+id/btn_issuestwo"
                app:layout_constraintBottom_toBottomOf="@id/bottomGuideline"
                android:background="@color/caper_six"
                app:layout_constraintVertical_bias="0.14"
                />

</androidx.constraintlayout.widget.ConstraintLayout>